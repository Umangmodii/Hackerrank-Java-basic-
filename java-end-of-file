import java.util.*;
import java.util.Scanner;

public class Solution
{
   public static void main(String[] args)
 {
   Scanner scn = new Scanner(System.in);

    int c = 1;

   while(scan.hasNext())
   {
        String s = scan.nextLine();
        System.out.println(c + " " + s);
        c++;
   }
 }
}


